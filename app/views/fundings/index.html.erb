<h1>Listing fundings</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Company perma</th>
    <th>Funding code</th>
    <th>Funding amount</th>
    <th>Funding currency</th>
    <th>Funding date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fundings.each do |funding| %>
  <tr>
    <td><%= funding.company_id %></td>
    <td><%= funding.company_perma %></td>
    <td><%= funding.funding_code %></td>
    <td><%= funding.funding_amount %></td>
    <td><%= funding.funding_currency %></td>
    <td><%= funding.funding_date %></td>
    <td><%= link_to 'Show', funding %></td>
    <td><%= link_to 'Edit', edit_funding_path(funding) %></td>
    <td><%= link_to 'Destroy', funding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Funding', new_funding_path %>
